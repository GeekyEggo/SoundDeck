{
    "name": "sounddeck",
    "version": "1.0.0",
    "description": "Sound capturing, buffering, and sound deck for Elgato Stream Deck.",
    "scripts": {
        "install": "npm run kill-esd && npm run install-plugin && npm run install-pi",
        "kill-esd": "tasklist | find /i \"StreamDeck.exe\" && taskkill /im StreamDeck.exe /f /t || echo Stream Deck not running",
        "install-plugin": "if not exist %APPDATA%\\Elgato\\StreamDeck\\Plugins\\com.geekyeggo.sounddeck.sdPlugin (npm run plugin-fresh) else (npm run plugin-update)",
        "install-pi": "npx webpack --config .\\src\\SoundDeck.PI\\webpack.config.js --mode development --display-error-details ",
        "plugin-fresh": "dotnet publish .\\src\\SoundDeck.Plugin\\ -c Debug -r win10-x64 -restore --no-build --output %APPDATA%\\Elgato\\StreamDeck\\Plugins\\com.geekyeggo.sounddeck.sdPlugin\\",
        "plugin-update": "xcopy /e /i /y .\\src\\SoundDeck.Plugin\\bin\\Debug\\netcoreapp3.0 %APPDATA%\\Elgato\\StreamDeck\\Plugins\\com.geekyeggo.sounddeck.sdPlugin\\",
        "watch-pi": "npx webpack --config .\\src\\SoundDeck.PI\\webpack.config.js --mode development --watch"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/GeekyEggo/SoundDeck.git"
    },
    "keywords": [
        "elgato-stream-deck",
        "audio",
        "sound-deck",
        "capture"
    ],
    "author": "Richard Herman",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/GeekyEggo/SoundDeck/issues"
    },
    "homepage": "https://github.com/GeekyEggo/SoundDeck#readme",
    "devDependencies": {
        "@babel/core": "^7.4.5",
        "@babel/preset-react": "^7.0.0",
        "babel-loader": "^8.0.6",
        "clean-webpack-plugin": "^3.0.0",
        "copy-webpack-plugin": "^5.0.3",
        "react": "^16.8.6",
        "react-dom": "^16.8.6",
        "react-redux": "^7.0.3",
        "redux": "^4.0.1",
        "webpack": "^4.32.2",
        "webpack-cli": "^3.3.2"
    },
    "dependencies": {}
}

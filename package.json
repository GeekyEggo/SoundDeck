{
    "name": "sounddeck",
    "version": "1.0.0",
    "description": "Sound capturing, buffering, and sound deck for Elgato Stream Deck.",
    "paths": {
        "debug": "bin\\Debug\\netcoreapp3.0",
        "dist":  ".\\dist",
        "elgato": "Elgato\\StreamDeck\\Plugins\\com.geekyeggo.sounddeck.sdPlugin",
        "plugin": ".\\src\\SoundDeck.Plugin"
    },
    "scripts": {
        "install": "npm stop && npm run install:plugin && npm run install:pi",
        "install:pi": "npx webpack",
        "install:plugin": "dotnet publish %npm_package_paths_plugin% -o %APPDATA%\\%npm_package_paths_elgato% -c Release -r win10-x64",
        "dist:plugin": "dotnet publish %npm_package_paths_plugin% -o %npm_package_paths_dist% -c Release -r win10-x64 --self-contained true /p:PublishTrimmed=true",
        "debug:pi": "npx webpack --mode development --watch",
        "debug:plugin": "xcopy /e /i /y %npm_package_paths_plugin%\\%npm_package_paths_debug% %APPDATA%\\%npm_package_paths_elgato%",
        "stop": "tasklist | findstr \"StreamDeck.exe\" && taskkill -im StreamDeck.exe -f -t || echo Stream Deck not running"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/GeekyEggo/SoundDeck.git"
    },
    "keywords": [
        "elgato-stream-deck",
        "audio",
        "sound-deck",
        "capture"
    ],
    "author": "GeekyEggo",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/GeekyEggo/SoundDeck/issues"
    },
    "homepage": "https://github.com/GeekyEggo/SoundDeck#readme",
    "devDependencies": {
        "@babel/core": "^7.4.5",
        "@babel/preset-react": "^7.0.0",
        "array-move": "^2.1.0",
        "babel-loader": "^8.0.6",
        "clean-webpack-plugin": "^3.0.0",
        "copy-webpack-plugin": "^5.0.3",
        "file-loader": "^4.0.0",
        "react": "^16.8.6",
        "react-dom": "^16.8.6",
        "react-redux": "^7.0.3",
        "react-sharpdeck": "file:lib/react-sharpdeck",
        "react-sortable-hoc": "^1.9.1",
        "redux": "^4.0.1",
        "style-loader": "^0.23.1",
        "webpack": "^4.32.2",
        "webpack-cli": "^3.3.6"
    },
    "dependencies": {
        "object-path": "^0.11.4"
    }
}

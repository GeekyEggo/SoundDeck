{
    "name": "sounddeck",
    "version": "1.0.0",
    "description": "Sound capturing, buffering, and sound deck for Elgato Stream Deck.",
    "paths": {
        "debug": "bin\\Debug",
        "dist": "./dist/com.geekyeggo.sounddeck.sdPlugin",
        "elgato": "Elgato\\StreamDeck\\Plugins\\com.geekyeggo.sounddeck.sdPlugin",
        "package": "./dist",
        "pi": ".\\src\\SoundDeck.PI",
        "plugin": ".\\src\\SoundDeck.Plugin.Windows"
    },
    "scripts": {
        "install": "MSBuild.exe -p:Configuration=Debug -t:Rebuild",
        "build:pi": "npx webpack",
        "build:release": "MSBuild.exe -p:Configuration=Release -t:Rebuild",
        "dist": "npm run dist:clean && npm run build:release && npm run dist:package",
        "dist:clean": "rm -rf %npm_package_paths_package%/*",
        "dist:package": "cd tools && DistributionTool -b -i ../%npm_package_paths_dist% -o ../%npm_package_paths_package% || exit 0",
        "dist:pi": "npx webpack --dist",
        "debug:pi": "npx webpack --mode development --watch",
        "debug:plugin": "xcopy /e /i /y %npm_package_paths_plugin%\\%npm_package_paths_debug% %APPDATA%\\%npm_package_paths_elgato%",
        "stop": "tasklist | findstr \"StreamDeck.exe\" && taskkill -im StreamDeck.exe -f -t || echo Stream Deck not running"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/GeekyEggo/SoundDeck.git"
    },
    "keywords": [
        "elgato-stream-deck",
        "audio",
        "sound-deck",
        "capture"
    ],
    "author": "GeekyEggo",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/GeekyEggo/SoundDeck/issues"
    },
    "homepage": "https://github.com/GeekyEggo/SoundDeck#readme",
    "devDependencies": {
        "@babel/core": "^7.4.5",
        "@babel/preset-react": "^7.0.0",
        "array-move": "^2.1.0",
        "babel-loader": "^8.0.6",
        "clean-webpack-plugin": "^3.0.0",
        "copy-webpack-plugin": "^5.0.3",
        "file-loader": "^4.0.0",
        "react": "^16.8.6",
        "react-dom": "^16.8.6",
        "react-redux": "^7.0.3",
        "react-sharpdeck": "file:lib/react-sharpdeck",
        "react-sortable-hoc": "^1.9.1",
        "redux": "^4.0.1",
        "style-loader": "^0.23.1",
        "webpack": "^4.32.2",
        "webpack-cli": "^3.3.6"
    },
    "dependencies": {
        "object-path": "^0.11.4"
    }
}
